{
	"name": "groq-code-cli",
	"version": "1.0.2",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/build-with-groq/groq-code-cli.git"
	},
	"bin": {
		"groq": "dist/core/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "ava",
		"install:global": "npm run build && npm link",
		"dev:auto": "tsx scripts/dev-auto.ts",
		"relink": "npm link",
		"release": "semantic-release",
		"release:dry-run": "semantic-release --dry-run"
	},
	"files": [
		"dist",
		"scripts"
	],
	"dependencies": {
		"chalk": "^5.4.1",
		"commander": "^14.0.0",
		"groq-sdk": "^0.27.0",
		"ink": "^6.0.1",
		"meow": "^11.0.0",
		"react": "^19.1.0"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.4",
		"@semantic-release/npm": "^12.0.2",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^19.1.8",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"chokidar": "^3.6.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"semantic-release": "^24.2.7",
		"ts-node": "^10.9.1",
		"tsx": "^4.19.2",
		"typescript": "^5.0.3",
		"xo": "^1.2.1"
	},
	"ava": {
		"files": [
			"test/**/*",
			"!test/helpers/**/*"
		],
		"extensions": {
			"ts": "module",
			"tsx": "module",
			"js": true
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"prettier": true,
		"semicolon": true,
		"ignores": [
			"test/**/*",
			"dist/**/*"
		]
	},
	"prettier": "@vdemedes/prettier-config"
}
